{
  "theme_name": "default",
  "font_name": "Menlo-Regular",
  "font_size": 16,
  "font_color_hex": "",
  "highlight": [
    {
      "name": "number",
      "pattern": "\\d+(\\.\\d+)?",
      "font_color_hex": ""
    },
    {
      "name": "command",
      "pattern": "\\\\([a-zA-Z]+|\\||\\{|\\}|&|%|_|#|\\$|\\\\)",
      "font_color_hex": ""
    },
    {
      "name": "curly_brackets",
      "pattern": "(?<!\\\\)(\\{|\\})",
      "font_color_hex": ""
    },
    {
      "name": "square_brackets",
      "pattern": "\\[|\\]",
      "font_color_hex": ""
    },
    {
      "name": "parentheses",
      "pattern": "\\(|\\)",
      "font_color_hex": ""
    }
  ]
}
